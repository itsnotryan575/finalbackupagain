{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/89E7AE4A-7DEE-4D53-8974-411BDA048E8D.JPEG",
    "splash": {
      "image": "./assets/images/89E7AE4A-7DEE-4D53-8974-411BDA048E8D.JPEG",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "armi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/89E7AE4A-7DEE-4D53-8974-411BDA048E8D.JPEG",
          "color": "#000000",
          "sounds": ["./assets/sounds/notification.wav"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}